public class HomePageViewModel : BaseViewModel
{
    private ObservableCollection<AssetGroup> assetGroups;
    public ObservableCollection<AssetGroup> AssetGroups
    {
        get { return assetGroups; }
        set { SetProperty(ref assetGroups, value); }
    }

    public HomePageViewModel()
    {
        AssetGroups = new ObservableCollection<AssetGroup>()
        {
            new AssetGroup { Name = "Cars", Count = 2, Icon = "car.png" },
            new AssetGroup { Name = "Trucks", Count = 3, Icon = "truck.png" },
            new AssetGroup { Name = "Generators", Count = 1, Icon = "generator.png" },
            new AssetGroup { Name = "Fridges", Count = 3, Icon = "fridge.png" }
        };
    }
}

public class AssetGroup
{
    public string Name { get; set; }
    public int Count { get; set; }
    public string Icon { get; set; }
}
